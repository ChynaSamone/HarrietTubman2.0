<script src="processing.js"></script>
<script type="text/processing" data-processing-target="mycanvas">  <!--city scroller-->
			/* @pjs preload="harriet.png"; */
			PImage Bharriet;
			var scroller;
			var building;
			
			void setup(){
			  scroller = new Scroller();
			  size(900, 450); <!--(WIDTH, HEIGHT)-->
			  background(32, 34, 75);
			  Bharriet = loadImage("harriet.png");
			}

			void draw(){  
			  background(32, 34, 75);
			  scroller.moveBuildingList();
			  scroller.drawBuildingList();
			  fill (68,46,24);
			  rect(0,420,900,50);
			  image(Bharriet, 0, 300, 120/2, 270/2);
			 }
			
			class Building {
				var height;
				var width;
				var xPos;
				var yPos;
				var xSpeed;
				
				Building(col, v, x, w) {
					r = col[0];
					g = col[1];
					b = col[2];
					xSpeed = v;
					height = random(100, 300);
					xPos = x;
					yPos = 450 - height;
					width = w;
				}
				
				void moveBuilding() {
					xPos = xPos + xSpeed;
				}
				
				void drawBuilding() {
					noStroke();
					fill(r, g, b);
					rect(xPos, yPos, width, height);
					
				}
			}
			
			class Scroller {
				var furthestX = 0;
				var buildingList = new Array();
				var xSpeed = -1;
				Scroller() {
					buildingList = [];
					while (furthestX < 900){
						var newWidth = random(50, 200);
						buildingList.push(new Building([46, 92, 45], xSpeed, furthestX, newWidth));
						furthestX = furthestX + newWidth;
					}
					
				}
				void drawBuildingList() {
					for (var i = 0; i < buildingList.length; i++) {
						buildingList[i].drawBuilding();  
					}
				}

				void moveBuildingList() {
					for (var i = 0; i < buildingList.length; i++) {
						buildingList[i].moveBuilding();
					}
					furthestX = furthestX + xSpeed;
					if (furthestX < 900) {
						var newWidth = random(50, 200);
						buildingList.push(new Building([46, 92, 45], xSpeed, furthestX, newWidth));
						furthestX = furthestX + newWidth;
					}
				}
				
			}
				
			
	</script>
</head>

